<div class="main-container">
  <table class="table">
    <thead>
      <tr>
      </tr>
    </thead>

    <tbody class="main-list">
      {{#each products}}
        <tr>
          <td class="main-pagetd">{{name}}</td>
          <td class="main-pagetd">{{description}}</td>
          <td class="main-pagetd"><img src="/uploads/{{images}}" /></td>
          <td>{{price}} z≈Ç</td>
          <td>
            <form action="/do-koszyka" method="POST">
              <input type="hidden" name="productId" value="{{_id}}" />
              <input type="number" name="quantity" value="1" min="1" />
              <button type="submit" class="btn btn-primary">Dodaj do koszyka</button>
            </form>
          </td>

        </tr>
      {{/each}}
    </tbody>
  </table>
</div>